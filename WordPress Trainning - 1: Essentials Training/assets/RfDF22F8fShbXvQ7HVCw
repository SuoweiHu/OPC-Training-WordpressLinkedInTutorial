			<script>
		let screenshot = document.querySelector('.screenshot')
		let wrapper = document.querySelector('.wrapper')
		let zoomable = false
		function setupZoom() {
			screenshot.addEventListener('load', () => {
				if (Math.abs(screenshot.naturalWidth - screenshot.width) > 2) {
					wrapper.classList.add('-zoomable')
					screenshot.classList.add('-zoomable')
					zoomable = true
					if (screenshot.naturalHeight / screenshot.naturalWidth > 1.5 && screenshot.naturalWidth < window.innerWidth) {
						wrapper.classList.add('-zoom')
						screenshot.classList.add('-zoom')
					}
				} else {
					wrapper.classList.remove('-zoomable')
					screenshot.classList.remove('-zoomable')
					zoomable = false
				}
			})
			window.addEventListener('resize', () => {
				wrapper.classList.remove('-zoom')
				screenshot.classList.remove('-zoom')
				if (Math.abs(screenshot.naturalWidth - screenshot.width) > 2) {
					wrapper.classList.add('-zoomable')
					screenshot.classList.add('-zoomable')
					zoomable = true
				} else {
					wrapper.classList.remove('-zoomable')
					screenshot.classList.remove('-zoomable')
					zoomable = false
				}
			})
			let startX = null
			let startY = null
			function mouseDown(event) {
				if (!zoomable) {
					return
				}
				startX = event.clientX || event.touches[0].clientX
				startY = event.clientY || event.touches[0].clientY
				screenshot.classList.add('-grabbing')
			}
			function mouseUp(event) {
				if (!zoomable) {
					return
				}
				let x = event.clientX || event.touches[0].clientX
				let y = event.clientY || event.touches[0].clientY
				if (Math.abs(startX - x) < 15 && Math.abs(startY - y) < 15) {
					wrapper.classList.toggle('-zoom')
					screenshot.classList.toggle('-zoom')
				}
				screenshot.classList.remove('-grabbing')
			}
			screenshot.addEventListener('mousedown', mouseDown)
			screenshot.addEventListener('touchdown', mouseDown)
			screenshot.addEventListener('mouseup', mouseUp)
			screenshot.addEventListener('touchup', mouseUp)
		}
		if (screenshot) {
			setupZoom()
		}
	</script>
	<script type="text/javascript">
		!function(e,n){"function"==typeof define&&define.amd?define(["exports"],n):"undefined"!=typeof exports?n(exports):n(e.dragscroll={})}(this,function(e){var n,t,o=window,r=document,i=[],l=function(e,l){for(e=0;e<i.length;)(l=(l=i[e++]).container||l).removeEventListener("mousedown",l.md,0),o.removeEventListener("mouseup",l.mu,0),o.removeEventListener("mousemove",l.mm,0);for(i=[].slice.call(r.getElementsByClassName("dragscroll")),e=0;e<i.length;)!function(e,i,l,s,d,m){(m=e.container||e).addEventListener("mousedown",m.md=function(n){e.hasAttribute("nochilddrag")&&r.elementFromPoint(n.pageX,n.pageY)!=m||(s=1,i=n.clientX,l=n.clientY,n.preventDefault())},0),o.addEventListener("mouseup",m.mu=function(){s=0},0),o.addEventListener("mousemove",m.mm=function(o){s&&((d=e.scroller||e).scrollLeft-=n=-i+(i=o.clientX),d.scrollTop-=t=-l+(l=o.clientY),e==r.body&&((d=r.documentElement).scrollLeft-=n,d.scrollTop-=t))},0)}(i[e++])};"complete"==r.readyState?l():o.addEventListener("load",l,0),e.reset=l});
	</script>
